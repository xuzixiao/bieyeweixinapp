.choosecitys{
  width: 94%;
  padding: 0 3%;
}
.city-title{
  display: inline-block;
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: #e7e7e7 solid 1px;
  color: #2b2b2b;
}
.choosecitys .city-title{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 40rpx;
}
.citylist{
  width: 100%;
  height: auto;
}
.citylist ul li {
  display: inline-block;
  width: 25%;
  text-align: center;
  line-height: 80rpx;
  font-size: 34rpx;
  margin-top: 10rpx;
}
.citylist li{
  width: 20%;
  height: auto;
}
.citylist li image{
  display: inline-block;
  width: 100%;
}

