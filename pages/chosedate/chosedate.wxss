.chosedate{
  widows: 96%;
  height: auto;
  margin: 0 2%;
}
.chosedate .weeks{
  display: flex;
  display: -webkit-flex;
  background: #fff;
  border-bottom:#f1f1f1 solid 1px ;
  color: #2b2b2b;
  width: 96%;
  height: 80rpx;
  position: fixed;
  top: 0;
  left: 2%;
  z-index: 5;
}
.chosedate .weeks text{
  width: 14.28%;
  text-align: center;
  font-size: 30rpx;
  line-height: 80rpx;
}
.chosedate .date-body{
 padding-top: 80rpx;
 background: #fff;
 padding-bottom: 200rpx;
}
.chosedate .date-body .date-mouth {
  margin-bottom:10px;
}
.date-mouth .mouth-title{
  text-align: center;
  font-size: 40rpx;
  line-height: 100rpx;
  background: #f1f1f1;
  color: #333;
} 
.date-mouth .days{
   display:flex;
   flex-wrap:wrap;
}
.date-mouth .days view{
  display: inline-block;
  width: 14.28%;
  text-align: center;
  font-size: 34rpx;
  line-height: 100rpx;
  height: 100rpx;
  color:#333 ;
}
.date-mouth .days view span{
  display:block;
  width: 100%;
  height: 50rpx;
  margin: 0px;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #2b2b2b;
}
.date-mouth .days view span.date{
  font-size: 34rpx;
}
.confirm{
  width: 100%;
  height:160rpx;
  position: fixed;
  left: 0;
  bottom: 0;
  background: #f1f1f1;
  z-index: 5;
  padding-top: 10rpx;
}
.confirm button{
  width: 80%;
  margin: 15rpx 10%;
  height: 70rpx;
  line-height: 70rpx;
  background: #f54449;
  color: #fff;
}
.days .startdate,.days .enddate{
  background: #f54449;
  color: #ffffff;
}
.days .startdate{
 border-radius: 30rpx 0px 0px 30rpx;
}
.days .enddate{
 border-radius: 0rpx 30rpx 30rpx 0rpx;

}
.days .startdate span{
  color: #fff;
}
.days .spacedate{
  background: #ffc5c7;
}
.expire{
  color:#bebebe!important;
}
.dateitem{
  position: relative;
}
.inorout{
  display: block;
  position: absolute;
  font-size: 26rpx!important;
  width: 100%!important;
  height: 50rpx!important;
  line-height: 50rpx!important;
  left: 0rpx!important;
  top: 0rpx!important;
  padding-top:50rpx; 
  text-align: center!important;
  background: #f54449;
  color: #fff!important;
  z-index: 3;
}
.dateday{
  font-size: 30rpx!important;
  width: 100%!important;
  height: 60rpx!important;
  position: absolute;
  top:0px!important;
  left:0px!important;
  text-align: center!important;
  line-height: 50rpx!important;
  z-index: 4; 
  }
  .ruzhu,.tuifang,.ing{
    color: #fff!important;
  }
  .ing{
  display: block;
  position: absolute;
  font-size: 20rpx!important;
  width: 100%!important;
  height: 50rpx!important;
  line-height: 50rpx!important;
  left: 0rpx!important;
  top: 0rpx!important;
  padding-top:50rpx; 
  text-align: center!important;
  color: #fff!important;
  z-index: 3;
  background: #ffc5c7;
  }
  .datebox{
    width: 96%;
    height: 60rpx;
    display: flex;
    padding: 0 2%;
    justify-content: center;
  }
  .datebox view{
    height: 60rpx;
    line-height: 60rpx;
    font-size: 26rpx;
    text-align: center;
    margin: 0 5px;
  }
.ingtext{
  color: #fff!important;
}
